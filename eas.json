      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_CRYPTO": "react-native-crypto",
        "EXPO_PUBLIC_WALLETCONNECT_ID": "9e1d3eb567d2f3e66db1368779dc9339"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
